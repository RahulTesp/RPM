<androidx.cardview.widget.CardView
    android:id="@+id/cardVitalHealthTrends30"
    android:layout_margin="@dimen/left_margin10dp"
    app:cardBackgroundColor="@color/white"
    android:layout_width="300dp"
    android:layout_height="wrap_content"
    app:cardCornerRadius="@dimen/margin_10dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="35dp"
        android:background="@color/Primary1">
        <TextView
            android:id="@+id/vitalname30"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/margin_5dp"
            android:layout_height="35dp"
            android:paddingLeft="10dp"
            android:text=""
            android:textColor="@color/Primary_dark"
            android:textSize="18dp"
            android:textStyle="normal" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="@dimen/margin_50dp"
        android:layout_marginRight="@dimen/spacing_xmedium"
        android:layout_marginLeft="@dimen/spacing_xmedium"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal">

        <com.github.mikephil.charting.charts.LineChart
            android:id="@+id/linechart30"
            android:layout_width="300dp"
            android:background="@color/white"
            android:layout_height="350dp"
            android:layout_marginTop="10dp"
            android:layout_margin="@dimen/left_margin10dp" />
    </LinearLayout>
</androidx.cardview.widget.CardView>